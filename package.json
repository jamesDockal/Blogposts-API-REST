{
  "name": "softaliza",
  "version": "1.0.0",
  "description": "An REST API to create blog posts. <br>\r Using `express`, `typescript` and `typeorm` with a `sqlite` `database`.",
  "main": "index.js",
  "engines": {
    "node": "14.17.3"
  },
  "scripts": {
    "build": "tsc",
    "start": "node src/server.js",
    "start:dev": "tsnd --transpile-only --ignore-watch __tests__ --ignore-watch node_modules --respawn typescript/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "prepare": "husky install",
    "test": "jest",
    "tsc": "tsc",
    "postinstall": "npm run tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.4",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/supertest": "^2.0.11",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.8",
    "typeorm-cli": "^1.0.7",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/swagger-ui-express": "^4.1.3",
    "bcrypt": "^5.0.1",
    "commitizen": "^4.2.4",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.12.14",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "tsc": "^2.0.3",
    "typeorm": "^0.2.34"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesDockal/Blogposts-API-REST.git"
  },
  "bugs": {
    "url": "https://github.com/jamesDockal/Blogposts-API-REST/issues"
  },
  "homepage": "https://github.com/jamesDockal/Blogposts-API-REST#readme"
}
